<div class="row">
  <br>
  <br>
  <br>
  <div class="col-md-6 col-md-offset-3"> 
    <% if params[:plan] == '2' %>
      <h1 class="text-center">Premium Account Sign Up</h1>
      <p class="text-center">Go Premium to view users' contact information.</p>
    <% else %>
      <h1 class="text-center">Free Account Sign Up</h1>
      <p class="text-center">Go Free to gain Club access.</p>
    <% end %>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <div class="well">
    <h2>Sign up</h2>
      
      <% if params[:plan] == '2' %>
        <%= render "premium_form" %>
      <% else %>
        <%= render "free_form" %>
      <% end %>

      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>